{
"languageserver": {
  "coc-clangd": {
    "enable": true,
    "path":"/usr/bin/",
    "command": "clangd",
    "rootPatterns": ["compile_flags.txt", "compile_commands.json"],
    "filetypes": ["c", "cc", "cpp", "c++", "objc", "objcpp"],
    "fallbackFlags": ["-std=c++17"],
    "arguments":  [
	"--log=verbose",
	"-j=4",
	"--background-index",
	"--clang-tidy",
	"-std=c++17",
	"--fallback-style=LLVM",
	"--header-insertion=iwyu",
	"--inlay-hints=true"
	],
  },},
"diagnostic.virtualText": true,
"diagnostic.virtualTextCurrentLineOnly": false,
"diagnostic.autoRefresh": true,
"diagnostic.hintSign": "+>",
"diagnostic.highlightPriority": 4096,
"diagnostic.refreshOnInsertMode": true,
"diagnostic.virtualTextLimitInOneLine": 2,
"diagnostic.enableHighlightLineNumber": true,
"diagnostic.level": "warning",
"diagnostic.errorSign": "●",
"diagnostic.warningSign": "⚠",
"diagnostic.virtualTextAlign":"after",
"diagnostic.checkCurrentLine": true,
"rust-analyzer.enable": true,
"rust-analyzer.serverPath": "/home/kero/ra-multiplex/target/release/ra-multiplex",
"rust-analyzer.inlayHints.parameterHints.enable": true,
"rust-analyzer.inlayHints.chainingHints.enable": true,
"rust-analyzer.inlayHints.renderColons": true,
"rust-analyzer.rust-analyzer.rustfmt.rangeFormatting.enable": true,
"rust-analyzer.updates.prompt":true,
"rust-analyzer.cachePriming.numThreads":7,
"rust-analyzer.cargo.autoreload":true,
"rust-analyzer.completion.callable.snippets":"fill_arguments",
"rust-analyzer.restartServerOnConfigChange": true,
"rust-analyzer.completion.autoself.enable": true,
"rust-analyzer.completion.postfix.enable":true,
"rust-analyzer.highlightRelated.breakPoints.enable":true,
"rust-analyzer.highlightRelated.exitPoints.enable":true,
"rust-analyzer.highlightRelated.references.enable":true,
"rust-analyzer.highlightRelated.yieldPoints.enable":true,
"rust-analyzer.lens.enable":true,
"rust-analyzer.lens.references.trait.enable":true,
"rust-analyzer.lens.run.enable": true,
"snippets.ultisnips.pythonPrompt": false,
}
